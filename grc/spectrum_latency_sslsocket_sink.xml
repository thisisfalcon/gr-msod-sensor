<?xml version="1.0"?>
<block>
  <name>sslsocket_sink</name>
  <key>spectrum_latency_sslsocket_sink</key>
  <category>[spectrum_latency]</category>
  <import>import spectrum_latency</import>
  <make>spectrum_latency.sslsocket_sink($num_ch, $dest_host, $sensorLoc, $sensorSys, $sensor_id, $sensor_key, $center_freq, $bandwidth, $meas_duration, $atten)</make>
  <param>
    <name>Num_ch</name>
    <key>num_ch</key>
    <type>raw</type>
  </param>
  <param>
    <name>Dest_host</name>
    <key>dest_host</key>
    <type>raw</type>
  </param>
  <param>
    <name>Sensorloc</name>
    <key>sensorLoc</key>
    <type>raw</type>
  </param>
  <param>
    <name>Sensorsys</name>
    <key>sensorSys</key>
    <type>raw</type>
  </param>
  <param>
    <name>Sensor_id</name>
    <key>sensor_id</key>
    <type>raw</type>
  </param>
  <param>
    <name>Sensor_key</name>
    <key>sensor_key</key>
    <type>raw</type>
  </param>
  <param>
    <name>Center_Freq</name>
    <key>center_freq</key>
    <type>float</type>
  </param>
  <param>
    <name>Bandwidth</name>
    <key>bandwidth</key>
    <type>float</type>
  </param>
  <param>
    <name>Meas_duration</name>
    <key>meas_duration</key>
    <type>float</type>
  </param>
  <param>
    <name>Atten</name>
    <key>atten</key>
    <type>float</type>
  </param>
  <sink>
    <name>in</name>
    <type>byte</type>
    <vlen>$num_ch</vlen>
  </sink>
</block>
